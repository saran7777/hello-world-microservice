pipeline {
    agent any
    parameters {
        string(name: 'GIT_URL', defaultValue: 'https://github.com/saran7777/hello-world-microservice.git', description: 'URL of the Git repository')
    }
    stages {
        stage('Checkout') {
            steps {
                git credentialsId: 'git-jenkins-pat', url: "${params.GIT_URL}"
            }
        }
        ...
    }
    ...
}
